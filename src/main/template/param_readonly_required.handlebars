<td class='code required'>{{name}}</td>
<td>
    {{#if isBody}}
        <textarea class='body-textarea' readonly='readonly' placeholder='(required)' name='{{name}}'>{{default}}</textarea>
    {{else}}
        {{#if default}}
            {{default}}
        {{else}}
            (empty)
        {{/if}}
    {{/if}}
</td>
<td class="markdown">{{{description}}}</td>
<td>{{{paramType}}}</td>
<td><span class="model-signature"></span></td>
